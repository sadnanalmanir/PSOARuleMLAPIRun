<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
		<title>PSOA RuleML API Demo</title>
		<link type="text/css" href="css/custom-theme/jquery-ui-1.8.20.custom.css" rel="Stylesheet" />
		<script type="text/javascript" src="js/jquery-1.7.2.min.js"></script>
		<script type="text/javascript" src="js/jquery-ui-1.8.20.custom.min.js"></script>

		<script src="js/main.js" type="text/javascript"></script>

		<link rel="shortcut icon" href="http://jqueryui.com/jquery-wp-content/themes/jqueryui.com/i/favicon.ico">

		<link rel="stylesheet" href="http://jqueryui.com/jquery-wp-content/themes/jquery/css/base.css?v=1">
		<link rel="stylesheet" href="http://jqueryui.com/jquery-wp-content/themes/jqueryui.com/style.css">
		<link rel="pingback" href="http://jqueryui.com/xmlrpc.php" />
		<!--[if lt IE 7]><link rel="stylesheet" href="css/font-awesome-ie7.min.css"><![endif]-->

		<script src="http://jqueryui.com/jquery-wp-content/themes/jquery/js/modernizr.custom.2.6.2.min.js"></script>

		<script src="http://jqueryui.com/jquery-wp-content/themes/jquery/js/plugins.js"></script>

	</head>
	<body>

		<!--h1>PSOATransRun</h1-->

		<div id="tabs">
			<ul>
				<li>
					<a href="#tab-1">PSOA Rulebase</a>
				</li>
				<li>
					<a href="#tab-2">TPTP Rulebase</a>
				</li>
			</ul>
			<div id="tab-1">
				<textarea id="PSOAKB" rows="20" cols="80"></textarea>																												
				
 <!-- Links Right begin-->
				<div style = "clear: right; float: right; text-align: right;">
					
						<ul>
							<br />
							<div class="ui-button">
								<input id="example1Btn" type="button" value="Example 1" />
							</div>
							<br />
							<div class="ui-button">
								<input id="example2Btn" type="button" value="Example 2" />
							</div>
							<br />
							<div class="ui-button">
								<input id="example3Btn" type="button" value="Example 3" />
							</div>
							<br />
							<div class="ui-button">
								<input id="example4Btn" type="button" value="Example 4" />
							</div>
				</div>
				<!-- Links right end-->

				<br/>
				<div style="visibility:hidden">
					<input type="radio" name="translatorType" value="Direct" />
					Direct Translator
					<input type="radio" name="translatorType" value="TPTPASO" checked="checked" />
					TPTP-ASO-based Translator
					<br/>
				</div>
				<input id="transKBBtn" type="button" value="Translate Rulebase" />
				<input id="showSampleKBBtn" type="button" value="Sample Rulebase" />
				<input id="resetKBBtn" type="button" value="Reset" />
			</div>

			<div id="tab-2">
				<textarea id="tptpOutput" rows="18" cols="80"></textarea>
				<h3>Query</h3>
				<textarea id="PSOAQuery" rows="1" cols="80"></textarea>																												
				
				
				
				
				
				
				
 <br/>
				<input id="transQueryBtn" type="button" value="Translate Query" />
				<input id="runQueryBtn" type="button" value="Execute" />
			</div>
		</div>

	</body>
</html>
