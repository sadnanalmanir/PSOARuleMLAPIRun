<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE Document [
  <!ENTITY psoa  "http://www.w3.org/2011/psoa#">
  <!ENTITY xs   "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
]>

<Document xmlns="&psoa;">
  <payload>
   <Group>
    <groupelement>
     <Forall>
      <declare>
       <Var>var1</Var>
      </declare>
      <declare>
       <Var>var2</Var>
      </declare>
      <formula>
      <Subclass>
       <sub>
        <Var>var1</Var>
       </sub>
       <super>
        <Var>var2</Var>
       </super>
      </Subclass>
      </formula>
     </Forall>
    </groupelement>
   </Group>
  </payload>
</Document>
