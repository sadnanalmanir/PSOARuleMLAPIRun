<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE Document [
  <!ENTITY psoa  "http://www.w3.org/2011/psoa#">
  <!ENTITY xs   "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
]>

<Document xmlns="&psoa;">
  <payload>
   <Group>
    <groupelement>
    <Implies>
       <if>
     <Equal>
      <left>
       <Var>a</Var>
      </left>
      <right>
       <Var>b</Var>
      </right>
      </Equal>     
      </if>
       <then>    <!--in head, if more than one variable, casting error shown for compareTo() because of using TreeSet-->
     <Atom>		<!--but two variables work when Expr e.g., var_1 ## var_2 () as psoa is used in head-->
     <Member>
      <instance>
       <Const type="&psoa;iri">http://example.org/#b</Const>
      </instance>
      <class>
       <Const type="&psoa;iri">http://example.org/#b</Const>
      </class>
      </Member>
      <tuple>
       <Var>vartup1</Var>
       <Var>vartup2</Var>
      </tuple>      
     </Atom>
        </then>
      </Implies>
    </groupelement>
   </Group>
  </payload>
</Document>
