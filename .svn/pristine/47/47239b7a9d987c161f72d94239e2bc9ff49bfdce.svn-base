<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE Document [
  <!ENTITY psoa  "http://www.w3.org/2011/psoa#">
  <!ENTITY xs   "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
]>
<!--here declared variables are not present inside the atomic formula, so they won't be in presentation syntax-->
<Document xmlns="&psoa;">
  <payload>
   <Group>
    <groupelement>
    <Implies>
       <if>
     <Atom>
     <Member>
      <instance>
       <Const type="&psoa;iri">http://example.org/#a</Const>
      </instance>
      <class>
       <Const type="&psoa;iri">http://example.org/#b</Const>
      </class>
      </Member>
      <tuple>
       <Var>vartup1</Var>
       <Const type="&psoa;iri">http://example.org/#c</Const>
      </tuple>      
     </Atom>
      </if>
       <then>
<Exists>
<declare><Var>var1</Var></declare>
<formula>
	  <Subclass>
	    <sub>
	      <Const type="&psoa;iri">http://example.org/#c</Const>
	    </sub>
	    <super>
	      <Const type="&psoa;iri">http://example.org/#c</Const>
	    </super>
	  </Subclass>
	  </formula>
</Exists>
        </then>
      </Implies>
    </groupelement>
   </Group>
  </payload>
</Document>
